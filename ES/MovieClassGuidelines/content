	[li 
		" 
		[a class=intlink href=/1144M 
			"Metroid
		]a
		$LF
	]li
	[li 
		" 
		[a class=intlink href=/1462M 
			"Legend of Zelda: Link's Awakening DX
		]a
		$LF
	]li
