			[pre 
				[b 
					"Q
				]b
				" - Organizar por calidad de técnica
				$LF
				[b 
					"E
				]b
				" - Organizar por calidad de entretenimiento
				$LF
				[b 
					"Y
				]b
				" - Organizar por ambas
				$LF
				[b 
					"U
				]b
				" - Organizar del mas reciente al mas antiguo
				$LF
				[b 
					"V
				]b
				" - Organizar del mas antiguo al mas reciente
				$LF
				[b 
					"S
				]b
				" - Organizar del mas corto al mas largo
				$LF
				[b 
					"L
				]b
				" - Organizar del mas largo al mas corto
				$LF
				[b 
					"F
				]b
				" - Una película aleatoria
				$LF
			]pre
