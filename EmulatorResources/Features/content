	$LF
	(SetTableAttributes|pattern=@^N$@|style=background:#EEE)
	$LF
	(SetTableAttributes|pattern=@^\?$@|style=background:#EEF)
	$LF
	(SetTableAttributes|pattern=@\+@|style=background:#8F8)
	$LF
]p
