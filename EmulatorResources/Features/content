		[tr 
			[th 
				"Emulator or Format
			]th
			[td 
				" This emulator or movie format has already been supported and used for publication.
				$LF
			]td
		]tr
		[tr 
			[td 
				"Emulator or Format
			]td
			[td 
				" This emulator or movie format has not yet been supported and used for publication. 
				$LF
			]td
		]tr
		[tr 
			[td 
				"+
			]td
			[td 
				" Has this feature
				$LF
			]td
		]tr
		[tr 
			[td 
				"-
			]td
			[td 
				" Does not have this feature
				$LF
			]td
		]tr
		[tr 
			[td 
				"±
			]td
			[td 
				" This feature is partially implemented (or partially available)
				$LF
			]td
		]tr
		[tr 
			[td 
				"?
			]td
			[td 
				" Not known (Hopefully this will be gone after some more people have at the table)
				$LF
			]td
		]tr
		[tr 
			[td 
				"N
			]td
			[td 
				" This feature is not applicable to this emulator
				$LF
			]td
		]tr
	]tbody
]table
