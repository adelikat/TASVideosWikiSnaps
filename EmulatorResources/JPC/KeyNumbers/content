[h4 id=Explanationsofspecialvalues 
	" Explanations of special values
	$LF
]h4
[table 
	[tbody 
		[tr 
			[th 
				"Special
			]th
			[th 
				"Meaning
			]th
		]tr
		[tr 
			[td 
				"Not valid
			]td
			[td 
				"code is not valid to be sent from keyboard
			]td
		]tr
		[tr 
			[td 
				"Reserved by the emulator
			]td
			[td 
				"The emulator does not allow this code
			]td
		]tr
		[tr 
			[td 
				"Unknown
			]td
			[td 
				"This code is valid but does not correspond to any known key
			]td
		]tr
	]tbody
]table
[p 
	(SetTableAttributes|pattern=@Not valid$@|style=)
	$LF
	(SetTableAttributes|pattern=@Reserved by the emulator@|style=)
	$LF
	(SetTableAttributes|pattern=@Unknown$@|style=)
	$LF
]p
[p 
	"[
	[a id=1 
	]a
	[a href=#r1 
		"1
	]a
	"]
	" Only valid in some contexts. Instead of KEYEDGE event, PAUSE uses PAUSE event.
	$LF
]p
[p 
	"[
	[a id=2 
	]a
	[a href=#r2 
		"2
	]a
	"]
	" Reserved for SysRq.
	$LF
]p
[p 
	"[
	[a id=3 
	]a
	[a href=#r3 
		"3
	]a
	"]
	" Reserved for Break.
]p
