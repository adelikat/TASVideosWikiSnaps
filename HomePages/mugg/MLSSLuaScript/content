			[h3 id=Overview
 
				" Overview
				$LF
			]h3
			[p 
				"This shows a bunch of things to the left of the game screen.
				$LF
			]p
			[ul 
				[li 
					" Top
					$LF
					[ul 
						[li 
							" Framecount (white color)
							$LF
						]li
						[li 
							" Lag count (red color)
							$LF
						]li
						[li 
							" Input display
							$LF
						]li
						[li 
							" RNG (grey color)
							$LF
						]li
						[li 
							" Room ID (grey color)
							$LF
						]li
					]ul
				]li
			]ul
			[ul 
				[li 
					" Stats 
					$LF
					[ul 
						[li 
							" Level
							$LF
						]li
						[li 
							" Experience to level up
							$LF
						]li
						[li 
							" Health / Max Health
							$LF
						]li
						[li 
							" BP / Max BP
							$LF
						]li
						[li 
							" Pow
							$LF
						]li
						[li 
							" Speed
							$LF
						]li
						[li 
							" Stache
							$LF
						]li
					]ul
				]li
			]ul
			[ul 
				[li 
					" Bros Attack Uses 
					[br 
					]br
					" (Amount of uses needed to learn the advanced version)
					$LF
					[ul 
						[li 
							" Sp (Splash Bros)
							$LF
						]li
						[li 
							" Bn (Bounce Bros)
							$LF
						]li
						[li 
							" Ch (Chopper Bros)
							$LF
						]li
						[li 
							" Kn (Knockback Bros)
							$LF
						]li
						[li 
							" Sw (Swing Bros)
							$LF
						]li
						[li 
							" Cy (Cyclone Bros)
							$LF
						]li
						[li 
							" Fi (Fire Bros)
							$LF
						]li
						[li 
							" Th (Thunder Bros)
							$LF
						]li
					]ul
				]li
			]ul
			[ul 
				[li 
					" Action Commands 
					[br 
					]br
					" (The arrow indicates who is leading) 
					[br 
					]br
					" (The Yellow number indicates how many frames it took to switch)
					$LF
					[ul 
						[li 
							" Luigi Back, Mario Front
							$LF
						]li
						[li 
							" Mario Back, Luigi Front
							$LF
						]li
					]ul
				]li
			]ul
			[ul 
				[li 
					" Bottom
					$LF
					[ul 
						[li 
							" Mush Power (= Extra attack power granted when wearing a Mush Badge. See 
							[a class=intlink href=/GameResources/GBx/MarioAndLuigiSuperstarSaga#UsingMushbadges 
								"here
							]a
							".)
							$LF
