[h2 id=UnderratedTasesILike 
	"Underrated TASes I like
	$LF
]h2
[div class=p 
	"Totally not ripping off Samsara’s userpage or anything, and totally did not get the idea from Memory
	$LF
]div
[div class= 
	[div class= 
		[ul class=nav nav-tabs role=tablist 
			[li class=nav-item 
				[a class=active nav-link data-bs-toggle=tab href=#tabs-10455-0 
					"Minimize
				]a
			]li
			[li class=nav-item 
				[a class=nav-link data-bs-toggle=tab href=#tabs-10455-1 
					"Hot takes
