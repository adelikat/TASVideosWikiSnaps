			[li 
				"zlib 1.2.7 with custom optimizations
				$LF
			]li
			[li 
				"libbz2 1.0.6
				$LF
			]li
			[li 
				"Lua 5.1.4
				$LF
			]li
			[li 
				"libpng 1.2.10
				$LF
