[h3 id=RemoveLinesThatDonTContainAPhrase 
	" Remove lines that don't contain a phrase
	$LF
]h3
[ul 
	[li 
		" 
		[b 
			"Find what
		]b
		": 
		[code 
			"(?!^.*target.*$)^.+
		]code
		$LF
	]li
	[li 
		" 
		[b 
			"Replace with
		]b
		": 
		[em 
			"leave blank
		]em
		$LF
	]li
	[li 
		" 
		[b 
			"Search Mode
		]b
		": Regex
		$LF
	]li
	[li 
		" 
		[b 
			"Matches Newline
		]b
		": Unchecked
		$LF
	]li
]ul
[h3 id=RemoveLinesThatContainAPhrase 
	" Remove lines that contain a phrase
	$LF
]h3
[ul 
	[li 
		" 
		[b 
			"Find what
		]b
		": 
		[code 
			".*target.*\r?\n
		]code
		$LF
	]li
	[li 
		" 
		[b 
			"Replace with
		]b
		": 
		[em 
			"leave blank
		]em
		$LF
	]li
	[li 
		" 
		[b 
			"Search Mode
		]b
		": Regex
		$LF
	]li
	[li 
		" 
		[b 
			"Matches Newline
		]b
		": Unchecked
		$LF
	]li
]ul
[h3 id=RemoveEverythingBeforeAPhraseInEachLine 
	" Remove everything before a phrase in each line
	$LF
]h3
[ul 
	[li 
		" 
		[b 
			"Find what
		]b
		": 
		[code 
			".+(target)
		]code
		$LF
	]li
	[li 
		" 
		[b 
			"Replace with
		]b
		":
		$LF
		[ul 
			[li 
				" 
				[em 
					"leave blank
				]em
				" to remove everything including target
				$LF
			]li
			[li 
				" 
				[code 
					"\1
				]code
				" to remove everything excluding target
				$LF
			]li
		]ul
	]li
	[li 
		" 
		[b 
			"Search Mode
		]b
		": Regex
		$LF
	]li
	[li 
		" 
		[b 
			"Matches Newline
		]b
		": Unchecked
		$LF
	]li
]ul
[h3 id=RemoveEverythingAfterAPhraseInEachLineIncludingTarget 
	" Remove everything after a phrase in each line (including target)
	$LF
]h3
[ul 
	[li 
		" 
		[b 
			"Find what
		]b
		": 
		[code 
			"target.*
		]code
		$LF
	]li
	[li 
		" 
		[b 
			"Replace with
		]b
		": 
		[em 
			"leave blank
		]em
		$LF
	]li
	[li 
		" 
		[b 
			"Search Mode
		]b
		": Regex
		$LF
	]li
	[li 
		" 
		[b 
			"Matches Newline
		]b
		": Unchecked
	]li
]ul
