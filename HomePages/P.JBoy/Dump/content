[h4 id=CodeBreakerCodeInfo 
	" CodeBreaker Code Info
	$LF
]h4
[pre 
	"ID:		0000xxxx 000y	x: ROM CRC			y: 2 disables IRQs, 8 indicates CRC Exists
	$LF
	"Hook:		1xxxxxxx 000y	x: ROM offset			y: Code handler store address 0-7 (0x8000100+(y<<22)
	[b 
	]b
	")
	$LF
	"OR:		2xxxxxxx yyyy	x: RAM address			y: Value
	$LF
	"8-bit:	3xxxxxxx 00yy	x: RAM address			y: New value
	$LF
	"Slide:	4xxxxxxx yyyy	x: Starting RAM address		y: New value
	$LF
	"		YYYYzzzz XXXX	X: Address increment		Y: Value increment	z: Number of writes
	$LF
	"MemWrite:	5xxxxxxx zzzz	x: Starting RAM address		z: Number of writes
	$LF
	"		yyyyyyyy yyyy	y: New 8-bit values
	$LF
	"AND:		6xxxxxxx yyyy	x: RAM address			y: Value
	$LF
	"If =:		7xxxxxxx yyyy	x: RAM address			y: Value
	$LF
	"16-bit:	8xxxxxxx yyyy	x: RAM address			y: New value
	$LF
	"If !=:	Axxxxxxx yyyy	x: RAM address			y: Value
	$LF
	"If >:		Bxxxxxxx yyyy	x: RAM address			y: Value
	$LF
	"If <:		Cxxxxxxx yyyy	x: RAM address			y: Value
	$LF
	"If Input:	D0000020 yyyy	y: Input value
	$LF
	"	0001	A
	$LF
	"	0002	B
	$LF
	"	0004	Select
	$LF
	"	0008	Start
	$LF
	"	0010	Right
	$LF
	"	0020	Left
	$LF
	"	0040	Up
	$LF
	"	0080	Down
	$LF
	"	0100	R
	$LF
	"	0200	L
	$LF
	"Plus:		Exxxxxxx yyyy	x: RAM address			y: Signed value
	$LF
	"If AND:	Fxxxxxxx yyyy	x: RAM address			y: Value
	$LF
]pre
[h4 id=ActionReplayv1/v2CodeInfo 
	" Action Replay v1/v2 Code Info
	$LF
]h4
[pre 
	"8-bit:		0xxxxxxx 000000yy	x: RAM address		y: New value
	$LF
	"16-bit:		1xxxxxxx 0000yyyy	x: RAM address		y: New value
	$LF
	"32-bit:		2xxxxxxx yyyyyyyy	x: RAM address		y: New value
	$LF
	"ListWrite:		3000zzzz yyyyyyyy	y: New value		z: Number of writes
	$LF
	"			xxxxxxxx xxxxxxxx	x: RAM addresses
	$LF
	"ROM patch:		60xxxxxx a000yyyy	x: ROM offset>>1	y: New value
	$LF
	"	a: If set, patch is enabled before game starts, else code handler enables patch
	$LF
	"8-bit AR button:	8x1xxxxx 000000yy	x: RAM address		y: New value
	$LF
	"16-bit AR button:	8x2xxxxx 0000yyyy	x: RAM address		y: New value
	$LF
	"Slowdown:		80F00000 0000xxxx	x: Number of AR loops
	$LF
	"If:			Dxxxxxxx 0000yyyy	x: RAM address		y: Value
	$LF
	"ListIf:		E0zzyyyy 0xxxxxxx	x: RAM address		y: Value	z: Number of codes
	$LF
	"Hook:			Fxxxxxxx 00000a0b	x: ROM address
	$LF
	"ID:			yyyyyyyy 001DC0DE	y: ROM ID (AGB-xxxx in ASCII)
	$LF
	"	a: If set, AR must be off when game starts
	$LF
	"	b=1: Don't push r14 when BL'ing
	$LF
	"	b=2: Push r14 when BL'ing
	$LF
	"	b=3: Replace a 32-bit pointer
	$LF
]pre
[h4 id=ActionReplayv3CodeInfo 
	" Action Replay v3 Code Info
	$LF
]h4
[pre 
	"EOF:				00000000 00000000
	$LF
	"Slowdown:			00000000 0800xx00  x: Number of AR loops
	$LF
	"EndIf				00000000 40000000
	$LF
	"Else				00000000 60000000
	$LF
	"8-bit fill:			00xxxxxx zzzzzzyy  x: Start RAM address	y: New value	z: Number of writes
	$LF
	"16-bit fill:			02xxxxxx zzzzyyyy  x: Start RAM address	y: New value	z: Number of writes
	$LF
	"32-bit:			04xxxxxx yyyyyyyy  x: RAM address	y: New value
	$LF
	"Indirect 8-bit fill:		40xxxxxx zzzzzzyy  x: Start RAM address	y: New value	z: Number of writes
	$LF
	"Indirect 16-bit fill:		42xxxxxx zzzzyyyy  x: Start RAM address	y: New value	z: Number of writes
	$LF
	"Indirect 32-bit:		44xxxxxx yyyyyyyy  x: RAM address	y: New value
	$LF
	"8-bit plus:			80xxxxxx 000000yy  x: RAM address	y: Value
	$LF
	"16-bit plus:			82xxxxxx 0000yyyy  x: RAM address	y: Value
	$LF
	"32-bit plus:			84xxxxxx yyyyyyyy  x: RAM address	y: Value
	$LF
	"Hook:				C4xxxxxx 00000abc  x: ROM address
	$LF
	"ID:				yyyyyyyy 001DC0DE  y: ROM ID (AGB-xxxx in ASCII)
	$LF
	"	a: If set, AR switch must be off when game starts
	$LF
	"	b: Number of codes AR processes at once
	$LF
	"	c: If set, push r14 when BL'ing
	$LF
	"16-bit I/O:			C6xxxxxx 0000yyyy  x: I/O offset	y: New value
	$LF
	"32-bit I/O:			C7xxxxxx yyyyyyyy  x: I/O offset	y: New value
	$LF
	"If:				zzxxxxxx yyyyyyyy  x: RAM Address	y: Value	z: [Size]+[Operand]+[Action]
	$LF
	"	[Size]			[Operand]		[Action]
	$LF
	"	00 8-bit		08 =			00 Execute next code
	$LF
	"	02 16-bit		10 !=			40 Execute next two codes
	$LF
	"	04 32-bit		18 < signed		80 Execute following codes until Else or EndIf
	$LF
	"	06 0-bit		20 > signed		C0 Execute following codes
	$LF
	"				28 <
	$LF
	"				30 >
	$LF
	"				38 AND
	$LF
	"8-bit AR button:		00000000 10xxxxxx  x: RAM address
	$LF
	"				000000yy 00000000  y: New value
	$LF
	"16-bit AR button:		00000000 12xxxxxx  x: RAM address
	$LF
	"				0000yyyy 00000000  y: New value
	$LF
	"32-bit AR button:		00000000 14xxxxxx  x: RAM address
	$LF
	"				yyyyyyyy 00000000  y: New value
	$LF
	"ROM patch 0:			00000000 18xxxxxx  x: ROM offset>>1
	$LF
	"				0000yyyy 00000000  y: New value
	$LF
	"ROM patch 1:			00000000 1Axxxxxx  x: ROM offset>>1
	$LF
	"				0000yyyy 00000000  y: New value
	$LF
	"ROM patch 2:			00000000 1Cxxxxxx  x: ROM offset>>1
	$LF
	"				0000yyyy 00000000  y: New value
	$LF
	"ROM patch 3:			00000000 1Exxxxxx  x: ROM offset>>1
	$LF
	"				0000yyyy 00000000  y: New value
	$LF
	"8-bit slide:			00000000 80xxxxxx  x: Starting RAM address
	$LF
	"				000000yy YYzzXXXX  X: Address increment		y: New value	Y: Value increment	z: Number of writes
	$LF
	"16-bit slide:			00000000 82xxxxxx  x: Starting RAM address
	$LF
	"				0000yyyy YYzzXXXX  X: Address increment		y: New value	Y: Value increment	z: Number of writes
	$LF
	"32-bit slide:			00000000 84xxxxxx  x: Starting RAM address
	$LF
	"				yyyyyyyy YYzzXXXX  X: Address increment		y: New value	Y: Value increment	z: Number of writes
	$LF
]pre
