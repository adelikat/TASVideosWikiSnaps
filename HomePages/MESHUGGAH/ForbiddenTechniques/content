[h3 id=TechniquesConflictingWithRules 
	" Techniques conflicting with rules
	$LF
]h3
[div class= 
	[div class= 
		[ul class=nav nav-tabs role=tablist 
			[li class=nav-item 
				[a class=active nav-link data-bs-toggle=tab href=#tabs-6674-0 
					"Info
				]a
			]li
			[li class=nav-item 
				[a class=nav-link data-bs-toggle=tab href=#tabs-6674-1 
					"No tampering with the files the game is composed of
				]a
			]li
			[li class=nav-item 
				[a class=nav-link data-bs-toggle=tab href=#tabs-6674-2 
					"Password related
				]a
			]li
			[li class=nav-item 
				[a class=nav-link data-bs-toggle=tab href=#tabs-6674-3 
					"Arbitrary initial RAM
				]a
