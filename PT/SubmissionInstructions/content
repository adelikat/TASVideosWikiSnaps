[div class=p 
	"Seu 
	[em 
		"movie
	]em
	" vai aparecer em alguns lugares:
	$LF
]div
[ul 
	[li 
		" Na 
		(__wikiLink|displaytext=página de mudanças recentes|href=/Recentchanges)
		", que lista tudo que aconteceu com as páginas neste site.
		$LF
	]li
