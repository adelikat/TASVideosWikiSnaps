[h4 id=Sumamry 
	" Sumamry
	$LF
]h4
[ul 
	[li 
		"Emulator Used: FCEUX 2.1.4a
		$LF
	]li
	[li 
		"Needs Phred's Cool Punch-out! 2 Turbo IPS Patch available on 
		[a class=extlink href=http://www.angelfire.com/mp/masterphred/nesdown.html rel=nofollow 
			"Master Phred's Site
		]a
		$LF
	]li
