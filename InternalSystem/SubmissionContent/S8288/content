[h3 id=Level4BigBen 
	"Level 4 — Big Ben
	$LF
]h3
[blockquote 
	"Tags: Object Clip
	$LF
]blockquote
[div class= 
	[div class= 
		[ul class=nav nav-tabs role=tablist 
			[li class=nav-item 
				[a class=active nav-link data-bs-toggle=tab href=#tabs-19042-0 
					"Description
				]a
			]li
