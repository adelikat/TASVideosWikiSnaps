]p
[ul 
	[li 
		"Emulator: VBA Rerecording, 1.72 v20
		$LF
	]li
	[li 
		"Takes damage to save time
		$LF
	]li
	[li 
		"Manipulates luck
		$LF
	]li
	[li 
		"Aims for fastest time
		$LF
