[h4 
	" Tricks 
	$LF
]h4
[dl 
	[dt 
		"Faster running
	]dt
	[dd 
		"By alternating between left and left+right or up and up+down, Link walks faster.
		$LF
		[dt 
			"Narrow passage boosting
		]dt
		[dd 
			"In narrow passages Link can walk very fast.
			$LF
			[dt 
				"Moon-jumping
			]dt
			[dd 
				"This works because the game thinks Link is on a different floor than expected.
				$LF
				[dt 
					"Screen-scroll glitch
				]dt
				[dd 
					"By pressing left+right when entering a door Link will appear on the other side.
					$LF
				]dd
			]dd
		]dd
	]dd
