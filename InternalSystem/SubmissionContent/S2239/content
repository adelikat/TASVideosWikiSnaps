[h3 id=Gameobjectives 
	" Game objectives
	$LF
]h3
[ul 
	[li 
		" Emulator used: VBA 1.72 + Rerecording v19.3/v19.4/v19.5/v21.0-interim
		$LF
	]li
	[li 
		" Aims for fastest real-time
		$LF
	]li
	[li 
		" Trades time for entertainment
		$LF
	]li
	[li 
		" Manipulates luck
		$LF
	]li
	[li 
		" Takes damage to save time
		$LF
	]li
	[li 
		" And so on
		$LF
	]li
]ul
