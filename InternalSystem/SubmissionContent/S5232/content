	[b 
		"Game Objectives
	]b
	$LF
]h3
[ul 
	[li 
		" Emulator used: Bizhawk 1.11.4 / 1.11.7 (18:33 v2 TAS)
		$LF
	]li
]ul
[ul 
	[li 
		" Takes damage to save time
		$LF
	]li
]ul
[ul 
	[li 
		" Abuses programing errors
		$LF
	]li
]ul
[ul 
	[li 
		" Manipulates luck (To get hammer weapon, not frizbee or diamond)
		$LF
	]li
