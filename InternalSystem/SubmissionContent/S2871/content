[h2 id=General 
	"General
	$LF
]h2
[ul 
	[li 
		" Emulator used: JPC-RR r11 (should also sync in r11.1-r11.2).
		$LF
	]li
	[li 
		" No special settings
		$LF
	]li
	[li 
		" Complete all 6 levels (no warps, no passwords)
		$LF
	]li
	[li 
		" Does not use death
		$LF
	]li
	[li 
		" Aims for fastest time
		$LF
	]li
]ul
[h2 id=Aboutgame: 
	"About game:
	$LF
]h2
[div class=p 
	"Mario&Luigi is fanmade freeware PC Mario game.
	$LF
]div
