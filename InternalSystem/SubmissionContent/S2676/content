[h4 id=Stage04ClampTheMiddle 
	" Stage 04, Clamp the middle
	$LF
]h4
[table 
	[tbody 
		[tr 
			[td 
				"Shot 1
			]td
			[td 
				"RATE=08
			]td
			[td 
				"Pocket balls 1 and 2. Use ball 2 as a projectile. Score += 260, (1*8 + 2*9)*10
				$LF
			]td
		]tr
		[tr 
			[td 
				"Shot 2
			]td
			[td 
				"RATE=12
			]td
			[td 
				"Positioning only. Touch ball 7.
				$LF
			]td
		]tr
		[tr 
			[td 
				"Shot 3
			]td
			[td 
				"RATE=01
			]td
			[td 
				"Pocket balls 4, 5 and 6.
				$LF
			]td
		]tr
		[tr 
			[td 
				"Shot 4
			]td
			[td 
				"RATE=08
			]td
			[td 
				"Pocket balls 3 and 7. Score += 870, (3*8 + 7*9)*10
				$LF
			]td
		]tr
	]tbody
]table
