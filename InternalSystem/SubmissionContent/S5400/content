[h3 id=BOSSNOTES: 
	" BOSS NOTES:
	$LF
]h3
[ul 
	[li 
		" all bosses move and/or summon spawns on a timer
		$LF
	]li
	[li 
		" boss timers are affected by damage dealt to the boss
		$LF
	]li
	[li 
		" rng is easily manipulated with directional inputs during the right frames
		$LF
	]li
	[li 
		" some bosses it is faster to wait to deal damage as to not wait even longer for more ammo
		$LF
	]li
]ul
