[h4 id=EELS: 
	" EELS:
	$LF
]h4
[ul 
	[li 
		" 3 hp (2x)
		$LF
	]li
	[li 
		" spawns crabs (crab placement is rng)
		$LF
	]li
	[li 
		" crabs spawn when previous crabs leave screen
		$LF
	]li
	[li 
		" manipulate crabs to spawn them closer together
		$LF
	]li
	[li 
		" it seems the eels will move if you have not hit them for some time
		$LF
	]li
	[li 
		" i thought it would be fast to keep the eels in place but this fight proves otherwise
		$LF
	]li
	[li 
		" eels have very short iframes
		$LF
	]li
]ul
