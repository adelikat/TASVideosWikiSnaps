[ul 
	[li 
		" R+1 = MAP+1
		$LF
	]li
	[li 
		" G+1 = MAP+32, G+8 = X+1
		$LF
	]li
	[li 
		" B+1 = X+4,    B+8 = Direction-90
		$LF
	]li
	[li 
		" Map = R(0-31) + G(0-31)%8*32
		$LF
	]li
	[li 
		" X   = G(0,8,16,24)/8 + B(0-31)%8*4
		$LF
	]li
	[li 
		" Y   = BS(1-6) - 1
		$LF
	]li
	[li 
		" Direction = B(0,8,16,24) = Right, Down, Left, Up
		$LF
	]li
