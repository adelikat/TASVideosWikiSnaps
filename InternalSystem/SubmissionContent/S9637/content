[h3 id=GameObjectives 
	" Game objectives
	$LF
]h3
[ul 
	[li 
		" Color all 16 dinosaurs such that no two adjacent regions are the same color 
		$LF
	]li
	[li 
		" Do so using the smallest number of colors (which is 4)
		$LF
	]li
]ul
