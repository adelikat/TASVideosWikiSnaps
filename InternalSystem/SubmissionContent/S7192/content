[h2 id=MovieGoals 
	"Movie goals
	$LF
]h2
[ul 
	[li 
		"Emulator used: Bizhawk 2.6.3-dev (Syncs on BizHawk 2.6.1)
		$LF
	]li
	[li 
		"CD ID: SCUS-94426
		$LF
	]li
	[li 
		"Bios: SCPH5501.bin
		$LF
	]li
	[li 
		"Version: NTSC-U
		$LF
	]li
	[li 
		"Abuses Programming Errors
		$LF
