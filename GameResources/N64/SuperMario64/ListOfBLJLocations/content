[h4 id=BobombBattlefield 
	" Bobomb Battlefield
	$LF
]h4
[ul 
	[li 
		"On the elevators beside the wooden bridge closest to start
		$LF
	]li
	[li 
		"On the side of the short green "hill" with the red coin on it
		$LF
	]li
	[li 
		"On the small slope at the bottom the hole in the ground where the black balls fall down
		$LF
	]li
	[li 
		"On an arrow shaped sign pointing to the entrance of the fence surrounding the mountain
		$LF
	]li
]ul
[h4 id=Whomp'sFortress 
	" Whomp's Fortress
	$LF
]h4
[ul 
	[li 
		"On the stairs leading to the Piranha Plant with the red coin behind it
		$LF
	]li
	[li 
		"On the low ceiling the lowest Thwomp creates while it's on the ground
		$LF
	]li
	[li 
		"Between the ground and the lowest step along the Fortress wall
		$LF
	]li
	[li 
		"On the rising elevators leading up to the fortress (or to the King Whomp's platform)
		$LF
	]li
]ul
