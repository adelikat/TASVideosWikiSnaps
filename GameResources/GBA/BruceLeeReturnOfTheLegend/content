[h3 id=Techniques 
	"Techniques
	$LF
]h3
[h4 id=Input 
	"Input
	$LF
]h4
[ul 
	[li 
		"U : ↑
		$LF
	]li
	[li 
		"D : ↓
		$LF
	]li
	[li 
		"L : ←
		$LF
	]li
	[li 
		"R : →
		$LF
	]li
	[li 
		"A : Jump (
		[em 
			"Short Jump
		]em
		" to 
		[em 
			"Long Jump
		]em
		": Holding A*1~A*3; A*4; A*5; A*6; A*7)
		$LF
	]li
	[li 
		"B : Punch (
		[em 
			"Punching Speed
		]em
		" makes difference to the 
		[em 
			"Punch Intensity
		]em
		", which means 
		[em 
			"Slower Punch
		]em
		" will cause 
		[em 
			"more damage
		]em
		" than 
		[em 
			"Faster & Earlier Punch
		]em
		"——It may only take effect when trying breaking a door)
		$LF
	]li
	[li 
		"l : Defence
		$LF
	]li
	[li 
		"r : Kick
		$LF
	]li
	[li 
		"S : Start
		$LF
	]li
	[li 
		"s : Select (
		[em 
			"Stow away
		]em
		" or 
		[em 
			"equip
		]em
		" your weapon)
		$LF
	]li
]ul
[h4 id=Note 
	"Note
	$LF
]h4
[ul 
	[li 
		"_ : Blank frame, no input; Prioritized
		$LF
	]li
	[li 
		" *n : Keep holding, n ∈ N (No "*n" means pressing the key once or keep holding it are also acceptable)
		$LF
	]li
	[li 
		" *n
		[sup 
			"+
		]sup
		" : n ∈ N
		[sup 
			"+
		]sup
		" ("*n
		[sup 
			"+
		]sup
		"", others are the same as above)
		$LF
	]li
	[li 
		" *? : Not sure if you need to keep holding the key but usually needed
		$LF
	]li
	[li 
		" + : Pressed immediately after the previous key input, or just sequentially pressed leaving an ephemeral or evanescent interval
		$LF
	]li
	[li 
		" *
		"[a, b]
		" : Holding the key for a frames, a+1 frames, ... , b frames, or also b+1 frames, ... , +∞ frames. Notice that this is only to give a definite supremum and infimum
		$LF
	]li
	[li 
		" 
		"[ + ]
		" : Optional "+"
		$LF
	]li
	[li 
		" ( ) : Keep while pressing other keys
		$LF
	]li
	[li 
		" { } : Keep or discontinue while pressing other keys
		$LF
	]li
	[li 
		" { }*n : Inputting a set of keys continually or continuously, which means you could make a interval after each input
		$LF
	]li
]ul
