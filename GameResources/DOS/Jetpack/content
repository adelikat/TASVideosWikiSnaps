[p 
	"Technical:
	$LF
]p
[ul 
	[li 
		" sp/f - subpixels per frame.
		$LF
	]li
	[li 
		" sp/f^2 - subpixels per frame squared.
		$LF
	]li
	[li 
		" hspeed - Horizontal speed.
		$LF
	]li
	[li 
		" vspeed - Vertical speed.
		$LF
	]li
]ul
[h2 
	" Open Game-Related Questions 
	$LF
	[a id=heading-11272 
	]a
]h2
[p 
	"_(remove once answered and add answer to relevant section)_
	$LF
]p
[ul 
