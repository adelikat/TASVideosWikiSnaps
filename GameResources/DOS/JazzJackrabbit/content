		" Bird helper
		$LF
	]li
	[li 
		" Four-hit shield
		$LF
	]li
	[li 
		" Rapid fire
		$LF
	]li
]ul
[p 
	"The following upgrades are not carried over after finishing a stage:
	$LF
]p
[ul 
	[li 
		" One-hit shield
		$LF
	]li
	[li 
		" High jump
		$LF
	]li
]ul
