[h3 id=NewsFeedAggregators 
	" News / Feed aggregators
	$LF
]h3
[div class=p 
	"Any 
	[a href=https://en.wikipedia.org/wiki/RSS rel=noopener external nofollow 
		"RSS
	]a
	" supporting reader or plug-in for your application or website should support our various feeds.
	$LF
]div
[div class=p 
	"You can subscribe to a a feed for 
	[a class=intlink href=/Wiki.rss 
		"wiki changes
	]a
	", or 
	(__wikiLink|displaytext=several others|href=/XMLDataFeed)
	".
	$LF
]div
[div class=p 
	"Our feeds conform to 
	[a href=http://validator.w3.org/feed/docs/rss2.html rel=noopener external nofollow 
		"RSS 2
	]a
	" and 
	[a href=http://search.yahoo.com/mrss/ rel=noopener external nofollow 
		"Media RSS 1.5
	]a
	".
	$LF
]div
