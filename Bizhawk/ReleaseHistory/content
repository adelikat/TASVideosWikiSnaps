[h2 id=Bizhawk242 
	" BizHawk 2.4.2
	$LF
]h2
[ul 
	[li 
		" 
		[a href=https://github.com/TASEmulators/BizHawk/releases/download/2.4.2/BizHawk-2.4.2.zip rel=noopener external nofollow 
			"Windows binary
		]a
		$LF
	]li
	[li 
		" Released May 17, 2020
		$LF
	]li
