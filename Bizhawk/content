[h4 id=QuickStart 
	" Quick Start
	$LF
]h4
[ul 
	[li 
		" Download and install the 
		[a href=https://github.com/TASEmulators/BizHawk-Prereqs/releases rel=noopener external nofollow 
			"prerequisites
		]a
		$LF
	]li
	[li 
		" Unzip BizHawk into its own folder
		$LF
	]li
	[li 
		" Run EmuHawk.exe
		$LF
	]li
]ul
