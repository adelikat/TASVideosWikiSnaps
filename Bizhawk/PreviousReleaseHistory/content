[h2 id=Bizhawk232 
	" BizHawk 2.3.2
	$LF
]h2
[ul 
	[li 
		" Released June 18, 2019
		$LF
	]li
	[li 
		" 
		[a href=https://github.com/TASEmulators/BizHawk/releases/download/2.3.2/BizHawk-2.3.2.zip rel=noopener external nofollow 
			"Windows binary
		]a
		$LF
	]li
]ul
[ul 
	[li 
		" mGBA
		$LF
		[ul 
			[li 
				" Update to 0.7.2
				$LF
			]li
		]ul
	]li
]ul
[ul 
	[li 
		" Gambatte
		$LF
		[ul 
			[li 
				" Update core (
				[a href=https://github.com/TASEmulators/BizHawk/pull/1582 rel=noopener external nofollow 
					"#1582
				]a
				")
				$LF
			]li
		]ul
	]li
]ul
[ul 
	[li 
		" N64:
		$LF
		[ul 
			[li 
				" Update GLideN64 to release 4.0
				$LF
			]li
			[li 
				" Fix GLideN64 defaults (
				[a href=https://github.com/TASEmulators/BizHawk/issues/1478 rel=noopener external nofollow 
					"#1478
				]a
				")
				$LF
			]li
			[li 
				" Support GLideN64 halo removal option
				$LF
			]li
			[li 
				" Virtual pad no longer crashes with standard controller
				$LF
			]li
			[li 
				" Fix Custom resolution
				$LF
			]li
		]ul
	]li
]ul
[ul 
	[li 
		" NESHawk
		$LF
		[ul 
			[li 
				" SubNESHawk - experimental version of NESHawk with poll-based input and PPU-cyle level reset support (not meant for casual play) (
				[a href=https://github.com/TASEmulators/BizHawk/pull/1439 rel=noopener external nofollow 
					"#1439
				]a
				")
				$LF
			]li
			[li 
				" Update VRC7 (
				[a href=https://github.com/TASEmulators/BizHawk/issues/1527 rel=noopener external nofollow 
					"#1527
				]a
				")
				$LF
			]li
			[li 
				" Add PPU cycle to tracelog
				$LF
			]li
		]ul
	]li
]ul
[ul 
	[li 
		" QuickNES
		$LF
		[ul 
			[li 
				" Fix QuickNES Crashes in Latest Dev Build (
