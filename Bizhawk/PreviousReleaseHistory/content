[ul 
	[li 
		" EmuHawk
		$LF
		[ul 
			[li 
				" Call memory callbacks with addr, value (
				[a href=https://github.com/TASEmulators/BizHawk/pull/1462 rel=noopener external nofollow 
					"#1462
				]a
				")
				$LF
			]li
			[li 
				" Fix OpenAdvanced Open ROM
				$LF
			]li
			[li 
				" Link icon
				$LF
				[ul 
					[li 
						" Update link text to match link status
						$LF
					]li
					[li 
						" Allow toggling Link status from Status bar button (
						[a href=https://github.com/TASEmulators/BizHawk/issues/321 rel=noopener external nofollow 
							"#321
						]a
						")
						$LF
					]li
				]ul
			]li
			[li 
				" FFMPEG writer
				$LF
				[ul 
					[li 
						" Change all uses of 
						[code 
							"-c:a libvo_aac
						]code
						" to 
						[code 
							"-c:a aac
						]code
						" because the ffmpeg we're packaging now no longer supports the former
						$LF
					]li
					[li 
						" Fix ffmpeg webm support (fixes 
						[a href=https://github.com/TASEmulators/BizHawk/issues/1477 rel=noopener external nofollow 
							"#1477
						]a
						")
						$LF
					]li
				]ul
			]li
			[li 
				" APIHawk
				$LF
				[ul 
					[li 
						" Extend with a Lua-like system of API libraries (
						[a href=https://github.com/TASEmulators/BizHawk/pull/1419 rel=noopener external nofollow 
							"#1419
						]a
						")
						$LF
					]li
					[li 
						" External tools no longer crash on loading new ROM
						$LF
					]li
					[li 
						" Rebuild HelloWorld external tool, include its zipfile sources
						$LF
					]li
				]ul
			]li
			[li 
				" Ram Search
				$LF
				[ul 
					[li 
						" Fix go to address
						$LF
					]li
				]ul
			]li
			[li 
				" Debugger
				$LF
				[ul 
					[li 
						" Fix mouse wheel scrolling
						$LF
					]li
					[li 
						" Set PageUp/Pagedown keys to increment/decrement
						$LF
					]li
				]ul
			]li
			[li 
				" Hex Editor 
				$LF
				[ul 
					[li 
						" Fix misalignment when bytes contain 7F (
						[a href=https://github.com/TASEmulators/BizHawk/issues/1503 rel=noopener external nofollow 
							"#1503
						]a
						")
						$LF
					]li
				]ul
			]li
			[li 
				" Movies
				$LF
				[ul 
					[li 
						" When Pause on Movie End is chosen, go to finished mode instead of stopping
						$LF
					]li
					[li 
						" Fix exception when statrting from saveram multple times
						$LF
					]li
				]ul
			]li
			[li 
				" TAStudio
				$LF
				[ul 
