[ul 
	[li 
		" Failed to load plugin mupen64-plus-audio-bkm.dll, error code: 0x7E
		$LF
		[ul 
			[li 
				" Update your Intel GPU drivers. Possible workaround in BizHawk 1.11.2
				$LF
			]li
		]ul
	]li
]ul
[ul 
	[li 
		" Fresh bizhawk download runs but window is missing despite being in taskbar
		$LF
		[ul 
			[li 
				" This has happened on VirtualBox. Try running bizhawk --fullscreen and the un-fullscreening it.
			]li
		]ul
	]li
]ul
