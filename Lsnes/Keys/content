[h3 
	"Joystick pseudo-keys
	$LF
	[a id=heading-1081 
	]a
]h3
[ul 
	[li 
		" joystick
		[tt 
			"x
		]tt
		"button
		[tt 
			"y
		]tt
		": 
		[tt 
			"y
		]tt
		"th button on 
		[tt 
			"x
		]tt
		"th joystick.
		$LF
	]li
	[li 
		" joystick
		[tt 
			"x
		]tt
		"axis
		[tt 
			"y
		]tt
		"-: 
		[tt 
			"y
		]tt
		"th axis on 
		[tt 
			"x
		]tt
		"th joystick to left / up.
		$LF
	]li
	[li 
		" joystick
		[tt 
			"x
		]tt
		"axis
		[tt 
			"y
		]tt
		"+: 
		[tt 
			"y
		]tt
		"th axis on 
		[tt 
			"x
		]tt
		"th joystick to right / down.
		$LF
	]li
	[li 
		" joystick
		[tt 
			"x
		]tt
		"hat
		[tt 
			"y
		]tt
		"n: 
		[tt 
			"y
		]tt
		"th hat on 
		[tt 
			"x
		]tt
		"th joystick to up.
		$LF
	]li
	[li 
		" joystick
		[tt 
			"x
		]tt
		"hat
		[tt 
			"y
		]tt
		"e: 
		[tt 
			"y
		]tt
		"th hat on 
		[tt 
			"x
		]tt
		"th joystick to right.
		$LF
	]li
	[li 
		" joystick
		[tt 
			"x
		]tt
		"hat
		[tt 
			"y
		]tt
