		" ctrl, lctrl, rctrl: Control keys
		$LF
	]li
	[li 
		" alt, lalt, ralt: ALT keys.
		$LF
	]li
	[li 
		" shift, lshift, rshift: Shift keys.
		$LF
	]li
	[li 
		" meta, lmeta, rmeta: Meta keys.
		$LF
	]li
	[li 
		" num: Numlock (sticky)
		$LF
	]li
	[li 
		" caps: Capslock (sticky)
		$LF
	]li
	[li 
		" mode: Mode select.
		$LF
	]li
]ul
