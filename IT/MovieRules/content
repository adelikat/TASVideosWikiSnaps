[h4 
	" Sega Master System e Sega Game Gear
	$LF
	[a id=heading-37297 
	]a
]h4
[ul 
	[li 
		" File filmato 
		(__wikiLink|/BizHawk|BizHawk)
		" (
		[a class=intlink href=/Bizhawk/BKMFormat 
			".bkm/.tas
		]a
		" / 
		[a class=intlink href=/Bizhawk/BK2Format 
			".bk2
		]a
		").
		$LF
	]li
	[li 
		" File filmato Dega (
		[a class=intlink href=/EmulatorResources/MMV 
			"MMV
		]a
		") sono 
		[em 
			[a class=extlink href=http://en.wikipedia.org/wiki/Deprecation rel=nofollow 
				"deprecati
			]a
		]em
		" e 
		[b 
			"non
		]b
		" accettati, a meno che una 
		[a class=intlink href=/Nach/Continuances 
			"continuazione
		]a
		" sia stata accordata.
		$LF
	]li
]ul
