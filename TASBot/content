[h3 id=Timeline 
	" Timeline
	$LF
]h3
[ul 
	[li 
		[b 
			"2003
		]b
		$LF
		[ul 
			[li 
				" 11-20: 
				[a href=http://web.archive.org/web/20031203222907/http://soramimi.egoism.jp/emu.htm rel=noopener external nofollow 
					"Morimoto
				]a
				" creates the first widely distributed Tool-Assisted Speedrun (TAS), labeled as a 
				(__wikiLink|displaytext=Time Attack|href=/668M)
				" of 
				(__wikiLink|displaytext=SMB3|href=/SMB3TASHistory)
				".
				$LF
			]li
			[li 
				" 11-26: 
				[a class=intlink href=/Users/Profile/Bisqwit 
					"Bisqwit
				]a
				" discovers a .WMV of Morimoto's SMB3 time attack, 
				(__wikiLink|displaytext=eventually leading him to create the website that became TASVideos.org|href=/SiteHistory)
				".
				$LF
			]li
		]ul
	]li
	[li 
		[b 
			"2006
		]b
		$LF
		[ul 
			[li 
				" 08-07: 
				[a class=intlink href=/Users/Profile/Ash Williams 
					"Ash Williams
				]a
				" comes up with the concept for a 
				(__wikiLink|displaytext="tas-bot" AI that learns how to play games|href=/Forum/Topics/4178)
				" which later turns into a 
				[a href=https://github.com/tizbac/tasbot rel=noopener external nofollow 
					"2012-12-29 Github project by the same name
				]a
				" similar to the more recent 
				[a href=http://pastebin.com/ZZmSNaHX rel=noopener external nofollow 
					"MarI/O
				]a
				" 
				(__wikiLink|displaytext=project|href=/Forum/Topics/16802)
				".
				$LF
			]li
			[li 
				" 08-27: 
				[a class=intlink href=/Users/Profile/Myria 
					"Myria
				]a
				" starts a discussion thread on the feasibility of 
				(__wikiLink|displaytext=playing back Tool-Assisted Speedruns on real hardware|href=/Forum/Topics/4288)
				".
				$LF
			]li
		]ul
	]li
	[li 
		[b 
			"2008
		]b
		$LF
		[ul 
			[li 
				" 10-20: 
				[a class=intlink href=/Users/Profile/true 
					"true
				]a
				" 
				(__wikiLink|displaytext=suggests|href=/Forum/Posts/182435)
				" 
				[em 
					""With typical frame-by-frame input, if the emulator is accurate, a movie could possibly be replayed on a real console."
				]em
				$LF
			]li
		]ul
	]li
	[li 
		[b 
			"2009
		]b
		$LF
		[ul 
			[li 
				" 10-14: 
				[a class=intlink href=/Users/Profile/true 
					"true
				]a
				" creates the first working controller interface 
				(__wikiLink|displaytext=using a PIC microcontroller|href=/Forum/Posts/261419)
				" which he uses to 
				(__wikiLink|displaytext=perform initial controller emulation testing on an NES|href=/Forum/Posts/216828)
				".
				$LF
			]li
			[li 
				" 11-02: Jaku creates a 
				[a href=https://www.parallax.com/product/bs2-ic rel=noopener external nofollow 
					"BS2
				]a
				" based replay device and successfully plays back prerecorded input of SMB1, 
				[a href=https://www.youtube.com/watch?v=f_Pr__d58f4 rel=noopener external nofollow 
					"completing the first level
				]a
				". 
				$LF
			]li
		]ul
	]li
	[li 
		[b 
			"2010
		]b
		$LF
		[ul 
			[li 
				" 07-16: 
				[a class=intlink href=/Users/Profile/micro500 
					"micro500
				]a
				" 
				(__wikiLink|displaytext=announces an NES replay device|href=/Forum/Posts/239984)
				" named NESBot.
				$LF
			]li
		]ul
	]li
	[li 
		[b 
			"2011
		]b
		$LF
		[ul 
			[li 
				" 02-10: 
				[a class=intlink href=/Users/Profile/micro500 
					"micro500
				]a
				" creates an 
				[a href=http://www.instructables.com/id/NESBot-Arduino-Powered-Robot-beating-Super-Mario-/ rel=noopener external nofollow 
					"Instructables guide
				]a
				" to build an NESBot using an Arduino Duemilanove microcontroller and a breadboard along with the first complete console verification run in the form of 
				(__wikiLink|displaytext=SMB|href=/1715M)
				".
				$LF
			]li
			[li 
				" 02-11: 
				[a class=intlink href=/Users/Profile/DarkKobold 
					"DarkKobold
				]a
				" suggests creating an icon and a 
				(__wikiLink|displaytext=special Verified category|href=/Movies-Verified)
				" for all movies that have been replayed on a console (
				(__wikiLink|displaytext=which adelikat refers to as "machine verified"|href=/Forum/Posts/261675)
				" at the time).
				$LF
			]li
			[li 
				" 07-07: 
				[a class=intlink href=/Users/Profile/DarkKobold 
					"DarkKobold
				]a
				" 
				[a href=https://gamesdonequick.com/tracker/run/987 rel=noopener external nofollow 
					"presents an NESBot demonstration
				]a
				" at 
				[a href=https://gamesdonequick.com/tracker/runs/3 rel=noopener external nofollow 
					"Summer Games Done Quick 2011
				]a
				", playing back 
				(__wikiLink|displaytext=SMB2|href=/1724M)
				" (
				[a href=https://archive.org/download/SummerGamesDoneQuick/SummerGamesDoneQuick_Part09_NESBot_SuperMarioBros2_HQ.mp4 rel=noopener external nofollow 
					"Archive.org encode
				]a
				") and 
				(__wikiLink|displaytext=Wizards And Warriors 3|href=/401M)
				" (
				[a href=https://archive.org/download/SummerGamesDoneQuick/SummerGamesDoneQuick_Part11_NESBot_WizardsAndWarriors3_HQ.mp4 rel=noopener external nofollow 
					"Archive.org encode
				]a
				") in front of ~1,500 viewers.
				$LF
			]li
			[li 
				" 11-12: 
				[a class=intlink href=/Users/Profile/SoulCal 
					"SoulCal
				]a
				" announces a TI MSP430-based 
				(__wikiLink|displaytext=N64 bot named Droid64|href=/Forum/Topics/12060)
				" that verifies 
				(__wikiLink|displaytext=SM64 0-stars|href=/1893M)
				" and 
				(__wikiLink|displaytext=120 stars|href=/850M)
				". 
				$LF
			]li
		]ul
	]li
	[li 
		[b 
			"2012
		]b
		$LF
		[ul 
			[li 
				" 03-01: 
				[a class=intlink href=/Users/Profile/micro500 
					"micro500
				]a
				" creates a Parallax Propeller-based 
				(__wikiLink|displaytext=N64 bot|href=/Forum/Posts/305411)
