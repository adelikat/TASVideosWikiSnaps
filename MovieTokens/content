	]div
	[div class=tab-content col-md-9 
		[div class=tab-pane fade active show id=tabs-114-0 
			[div class=p 
				"Examples:
				$LF
			]div
			[ul 
				[li 
					" 
					[code 
						"http://
						?IF 0
						?ENDIF 0
						"tasvideos.org/Movies-Token
					]code
					" single listing parameter
					$LF
				]li
				[li 
					" 
					[code 
						"http://
						?IF 0
						?ENDIF 0
						"tasvideos.org/Movies-Token-Token-Token-Token
					]code
					" - multiple parameters
					$LF
				]li
			]ul
			[pre 
				[b 
					"All
				]b
				$LF
				"<ClassName> - Filter by 
				(__wikiLink|href=/Class|implicitdisplaytext=class)
				$LF
				[b 
					"Y
				]b
				"<Year>
				$LF
				"<GenreName> - Filters by game genre
				$LF
				"<FlagName> - Filter by movie flag
				$LF
				[b 
					"Group
				]b
				"<GroupID> - Filter by 
				(__wikiLink|displaytext=game group|href=/GameGroups/List)
				$LF
				[b 
					"author
				]b
				"<AuthorID> - see 
				(__wikiLink|href=/Users/List|implicitdisplaytext=Users/List)
				" for IDs
				$LF
				"<SystemCode> - Filter by 
				(__wikiLink|displaytext=system|href=/Systems)
				$LF
				"<TagName>  - Filter by movie tag
				$LF
				[b 
					"Obs
				]b
				" - Include obsoleted
				$LF
				[b 
					"Obsonly
				]b
				" - Exclude current
				$LF
				[b 
					"Sort
				]b
				"<SortingID> - Sorting
				$LF
				"<MovieID>
				[b 
					"M
				]b
				" - Show the given publication
				$LF
				"<GameID>
				[b 
					"G
				]b
				" - Filter by a given 
				(__wikiLink|displaytext=game|href=/Games/List)
				$LF
				[b 
					"Limit
				]b
				"<N> - Limit the number of results
				$LF
			]pre
		]div
		[div class=tab-pane fade id=tabs-114-1 
			[div class=p 
				"Example: 
				[a href=http://tasvideos.org/Movies-puzzle rel=noopener external nofollow 
					"http://tasvideos.org/Movies-puzzle
				]a
				$LF
			]div
			[pre 
				"puzzle
				$LF
				"racing
				$LF
				"rpg
				$LF
				"strategy
				$LF
				"sport
				$LF
				"board
				$LF
				"gameshow
				$LF
				"unofficial game
				$LF
				"adventure
				$LF
				"shooter
				$LF
				"action
				$LF
				"storybook
				$LF
				"platformer
				$LF
				"party
				$LF
				"educational
				$LF
				"bootleg
				$LF
				"fangame
				$LF
				"homebrew
				$LF
				"hack
				$LF
				"custom level set
				$LF
				"unofficial port
				$LF
				"unlicensed game
				$LF
				"unreleased game
				$LF
				"rhythm
				$LF
				"prerelease
				$LF
				"application
				$LF
			]pre
		]div
		[div class=tab-pane fade id=tabs-114-2 
			[div class=p 
				"Example: 
				[a href=http://tasvideos.org/Movies-Atlas rel=noopener external nofollow 
					"http://tasvideos.org/Movies-Atlas
				]a
				$LF
			]div
			[pre 
				[b 
					"Atlas
					$LF
					"Commentary
					$LF
					"Firstplatform
					$LF
					"Knownrecords
					$LF
					"Newcomerrec
					$LF
					"Verified
				]b
				$LF
			]pre
		]div
		[div class=tab-pane fade id=tabs-114-3 
			[div class=p 
				"Filters the movie list by publication tag.
				$LF
			]div
			[div class=p 
				"Example: 
				[a href=http://tasvideos.org/Movies-play rel=noopener external nofollow 
					"http://tasvideos.org/Movies-play
				]a
				$LF
			]div
			[pre 
				"play                  - Playaround
				$LF
				"maxscore              - Aims for maximum score
				$LF
				"speed_tradeoff        - Contains speed/entertainment tradeoffs
				$LF
				"gametimer             - Aims for in-game time instead of real-time
				$LF
				"demo                  - Demonstration
				$LF
				"full                  - 100% completion
				$LF
				"nofull                - Low% completion
				$LF
				"kills                 - Maximum kills
				$LF
				"nokills               - Pacifist version
				$LF
				"bestending            - Best ending
				$LF
				"bigame                - Multiple games in one movie
				$LF
				"2p                    - 2 Players
				$LF
				"just1p                - One player in a multiplayer game
				$LF
				"3p                    - 3 Players
				$LF
				"4p                    - 4 Players
				$LF
				"5p                    - 5 Players
				$LF
				"subopt_char           - Uses a suboptimal character
				$LF
				"death                 - Uses intentional deaths
				$LF
				"nodeath               - Forgoes intentional deaths
				$LF
				"damage                - Takes intentional damage
				$LF
				"nodamage              - Forgoes intentional damage
				$LF
				"warps                 - Uses warps
				$LF
				"nowarps               - Forgoes warps
				$LF
				"warpbug               - Major skip glitch
				$LF
				"nowarpbug             - Forgoes major skip glitch
				$LF
				"skipboss              - Final boss skip glitch
				$LF
				"noskipboss            - Forgoes final boss skip glitch
				$LF
				"restart               - Uses a game restart sequence
				$LF
				"norestart             - Forgoes a time-saving restart sequence
				$LF
				"ace                   - Executes arbitrary code
				$LF
				"bugs                  - Heavy glitch abuse
				$LF
				"nobugs                - Forgoes time-saving glitches
				$LF
				"savebugs              - Corrupts save data
				$LF
				"nosavebugs            - Forgoes save data corruption
				$LF
				"rambugs               - Corrupts memory
				$LF
				"norambugs             - Foregoes memory corruption
				$LF
				"random                - Heavy luck manipulation
				$LF
				"initstate             - Uses uninitialized initial state
				$LF
				"1level                - Single level
				$LF
				"easy                  - Uses easiest difficulty
				$LF
				"noeasy                - Uses hardest difficulty
				$LF
				"second_quest          - Second quest/Post game-completion
				$LF
				"save                  - Starts from a saved state or SRAM
				$LF
				"password              - Uses a level password
				$LF
				"unintendedenviroment  - Abuses unintended environment
				$LF
				"ingamecode            - Uses an in-game code
				$LF
				"resources             - Heavy resource management
				$LF
			]pre
		]div
		[div class=tab-pane fade id=tabs-114-4 
			[div class=p 
				"Example: 
				[a href=http://tasvideos.org/Movies-SortU rel=noopener external nofollow 
					"http://tasvideos.org/Movies-SortU
				]a
				$LF
			]div
			[pre 
				[b 
					"U
				]b
				" - sort from newest to oldest
				$LF
				[b 
					"V
				]b
				" - sort from oldest to newest
				$LF
				[b 
					"S
				]b
				" - sort from shortest to longest
				$LF
				[b 
					"L
				]b
				" - sort from longest to shortest
				$LF
				[b 
					"Y
				]b
				" - sort by rating (highest to lowest)
				$LF
			]pre
		]div
	]div
]div
