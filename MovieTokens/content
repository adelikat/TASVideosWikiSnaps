					"Q
				]b
				" - sort by tech quality
				$LF
				[b 
					"E
				]b
				" - sort by entertainment
				$LF
				[b 
					"Y
				]b
				" - sort by combined rating
				$LF
				[b 
					"U
				]b
				" - sort from newest to oldest
				$LF
				[b 
					"V
				]b
				" - sort from oldest to newest
				$LF
				[b 
					"S
				]b
				" - sort from shortest to longest
				$LF
				[b 
					"L
				]b
				" - sort from longest to shortest
				$LF
				[b 
					"F
				]b
				" - 1 random movie
				$LF
			]pre
