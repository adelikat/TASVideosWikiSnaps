]pre
[p 
	"Lists statistics of movies.
	[br 
	]br
	$LF
	"If invoked without parameters, lists generic statistics.
	$LF
]p
[p 
	"Params:
	$LF
]p
[table 
	[tbody 
		[tr 
			[td 
				" comp=[-]fieldname 
			]td
			[td 
				" specifies field to compare using
				$LF
			]td
		]tr
		[tr 
			[td 
				" need=fieldname 
			]td
			[td 
				" specifies which field must have nonzero value
				$LF
			]td
		]tr
		[tr 
			[td 
				" fields=fieldname=fielddesc{,...} 
			]td
			[td 
				" lists fields to list
				$LF
			]td
		]tr
		[tr 
			[td 
				" top=n 
			]td
			[td 
				" specifies the number of results to output (default 10)
				$LF
			]td
		]tr
		[tr 
			[td 
				" minAge=n 
			]td
			[td 
				" specifies the minimum age of the movie in days to be listed
				$LF
			]td
		]tr
	]tbody
]table
[p 
	"List of available fields is subject to change.
	$LF
	"Currently it is:
	[br 
	]br
	$LF
	"id, length, filererecords, playerid, recommended, alength, asize, aname,
	$LF
	"desclen, num_lines, udesclen, formattedLength, formattedALength, compressionRatio, alengthPerLength, alengthMinusLength, formattedALengthMinusLength, btDownloaded, btDownloadedMegs, daysPublished, downloadsPerDay.
	$LF
]p
